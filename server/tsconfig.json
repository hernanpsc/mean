{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "ES2022",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "allowJs": true,
        "paths": {
            "*": ["node_modules/*"]
        },
        "types": [
            "node",
            "express",
          ]
    },
    "include": ["src/**/*"]
}
